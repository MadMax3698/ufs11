{
    "$schema": "https://schemas.wp.org/trunk/theme.json",
    "settings": {
        "appearanceTools": true,
        "color":{
            "defaultPalette":false,
            "defaultGradients": false,
            "defaultDuotone": false,
            "palette": [
                {
                    "slug": "Onyx",
                    "color": "#383d3b",
                    "name": "Dark grey"
                },
                {
                    "slug": "Lavander-blush",
                    "color": "#eee5e9",
                    "name": "Light grey"
                },
                {
                    "slug": "Grey",
                    "color": "#7c7c7c",
                    "name": "Grey"
                },
                {
                    "slug": "Non-photo-blue",
                    "color": "#92dce5",
                    "name": "Dark azure"
                },
                {
                    "slug": "Robin-egg-blue",
                    "color": "#52dee5",
                    "name": "Azure"
                }
            ]
        },
        "typography": {
            "fluid":true,
            "fontFamilies": [
                {
                    "fontFamily":"Roboto, san-serif",
                    "slug":"Roboto",
                    "name":"Roboto", 
                    "fontFace":[
