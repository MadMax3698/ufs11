                {
                    "slug": "m",
                    "size": "1.5rem",
                    "name": "Medium",
                    "fluid": {
                        "min": "1rem",
                        "max": "1.5rem"
                    }
                },
                {
                    "slug": "l",
                    "size": "2rem",
                    "name": "Large",
                    "fluid": {
                        "min": "1.5rem",
                        "max": "2rem"
                    }
                },
                {
                    "slug": "xl",
                    "size": "2.5rem",
                    "name": "Extra large",
                    "fluid": {
                        "min": "2rem",
                        "max": "2.5rem"
                    }
